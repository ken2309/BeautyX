ents\\CartList.tsx",[],["2703"],"C:\\Users\\longdc\\Desktop\\webbooking\\src\\features\\Cart\\components\\CartNull.tsx",[],[],"C:\\Users\\longdc\\Desktop\\webbooking\\src\\features\\Cart\\components\\CartPopupNoti.tsx",[],[],"C:\\Users\\longdc\\Desktop\\webbooking\\src\\features\\Cart\\components\\CartPopupNotiSign.tsx",[],[],"C:\\Users\\longdc\\Desktop\\webbooking\\src\\features\\Cart\\components\\PopupDiscountQuantity.tsx",[],[],"C:\\Users\\longdc\\Desktop\\webbooking\\src\\features\\Cart\\index.tsx",[],[],"C:\\Users\\longdc\\Desktop\\webbooking\\src\\features\\CartPayment\\components\\PaymentBranch.tsx",[],[],"C:\\Users\\longdc\\Desktop\\webbooking\\src\\features\\CartPayment\\components\\PaymentCart.tsx",[],[],"C:\\Users\\longdc\\Desktop\\webbooking\\src\\features\\CartPayment\\components\\PaymentCreateFail.tsx",[],[],"C:\\Users\\longdc\\Desktop\\webbooking\\src\\features\\CartPayment\\components\\PaymentForm.tsx",["2704","2705","2706","2707","2708"],[],"import React, { useContext, useState } from 'react';\r\nimport icon from '../../../constants/icon';\r\nimport PopupSuccess from '../../PopupSuccess/index';\r\nimport { useHistory } from 'react-router-dom'\r\nimport { AppContext } from '../../../context/AppProvider';\r\nimport onErrorImg from '../../../utils/errorImg';\r\nimport PaymentBranch from './PaymentBranch';\r\nimport { useSelector } from 'react-redux';\r\nimport UserPaymentInfo from '../../Account/components/UserPaymentInfo';\r\n\r\n//const phoneFormat = /^((\\\\+[1-9]{1,4}[ \\\\-]*)|(\\\\([0-9]{2,3}\\\\)[ \\\\-]*)|([0-9]{2,4})[ \\\\-]*)*?[0-9]{3,4}?[ \\\\-]*[0-9]{3,4}?$/\r\nfunction PaymentForm(props: any) {\r\n      const { setNote, list, chooseBr, setChooseBr, setAddress } = props;\r\n      const org = list[0]?.org;\r\n      const [open, setOpen] = useState(false);\r\n      const { t } = useContext(AppContext);\r\n      const history = useHistory();\r\n      const { address } = props;\r\n      const [popup, setPopup] = useState(false);\r\n      const 